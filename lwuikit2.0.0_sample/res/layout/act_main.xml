<?xml version="1.0" encoding="utf-8"?>

<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <Button 
            android:id="@+id/menu1"
            android:text="Lyric"
            android:onClick="onClick"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
        <Button 
            android:id="@+id/menu2"
            android:text="Smart Animation Fragment(saf)"
            android:onClick="onClick"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
        <Button 
            android:id="@+id/menu3"
            android:text="SlidingMenu"
            android:onClick="onClick"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
        <Button 
            android:id="@+id/menu4"
            android:text="alphabet"
            android:onClick="onClick"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
        <Button 
            android:id="@+id/menu5"
            android:text="Tip"
            android:onClick="onClick"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
        <Button 
            android:id="@+id/menu6"
            android:text="EcoGallery"
            android:onClick="onClick"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
        <Button 
            android:id="@+id/menu7"
            android:text="CyclePicker"
            android:onClick="onClick"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
        <Button 
            android:id="@+id/menu8"
            android:text="Bar"
            android:onClick="onClick"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
        <Button 
            android:id="@+id/menu9"
            android:text="WakeLock"
            android:onClick="onClick"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
        <Button 
            android:id="@+id/menu10"
            android:text="listview"
            android:onClick="onClick"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
    </LinearLayout>
</ScrollView>
